  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "start": "set PORT=3010&& react-scripts start",
    "watch": "watch \"yarn  build\" ./src ",
    "build": "set NODE_ENV=production&& tsc -build tsconfig.build.json && yarn copyfiles1",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist",
    "copyfiles1": "copyfiles package.json README.md dist/ && copyfiles --up 1 src/**/*.scss dist/",
    "yalc-pub": "yarn build && pushd dist && yalc publish --private & popd",
    "todo": "rmdir /S/Q dist & ",
    "xbuild": "react-scripts build",
    "test": "react-scripts test  --watchAll=false",
    "test-watch": "react-scripts test",
    "test-watch-debug": "jest --watch  --verbose --runInBand",
    "x eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
